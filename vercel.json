{
    "buildCommand": "npx expo export --platform web --output-dir web-build",
    "outputDirectory": "web-build",
    "devCommand": "npx expo start --web",
    "installCommand": "npm install --legacy-peer-deps",
    "framework": null,
    "rewrites": [
        {
            "source": "/(.*)",
            "destination": "/index.html"
        }
    ]
}